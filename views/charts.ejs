<html>
  <head>
   <script type="text/javascript">
   window.onload = function () {
     var chart = new CanvasJS.Chart("chartContainer", {
       theme:"theme3", //try theme1, theme2
       backgroundColor: "#F3F3F3",
       title:{
         text: "Basic Column Chart - CanvasJS" //Try changing             
       },
       data: [              
         {          
         // Change type to bar, area, line or pie
         type: "column",
         dataPoints: [
          { label: "apple", y: 14 },
          { label: "orange", y: 19 },
          { label: "banana", y: 25 },
          { label: "mango", y: 30 },
          { label: "grape", y: 34 },
          { label: "guava", y: 28 },
          { label: "pineapple", y: 24 },
         ]
        }
       ],
    });
    chart.render();
  }
</script>
<script type="text/javascript" src= "https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</head>
<body>
  <div id="chartContainer" style="height: 300px; width: 100%;">
  </div>
</body>
</html>